msgstr ""

msgid ""
"Timezone in <a href=\"https://en.wikipedia.org/wiki/"
"Tz_database\">\"tz\" (Olson) format</a>, eg. \"America/Los_Angeles\"."
msgstr ""

# Seat type (window, aisle, middle)
msgid "Type"
msgstr "Plats"

msgid ""
